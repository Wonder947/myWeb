{
    "version": 2,
    "name": "myweb",
    "builds": [
       { "src": "app.mjs", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/app.mjs" }
    ],
    "installCommand": "npm install",
    "headers": [
        {
            "source": "/app.mjs",
            "headers": [
                {
                    "view engine": "hbs"
                }
            ]
        }
    ]
 }
